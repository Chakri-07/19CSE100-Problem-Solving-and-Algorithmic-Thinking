    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="to find the modulus of a given number"/>
            <declare name="Divident, Divisor" type="Real" array="False" size=""/>
            <declare name="Quotient, remainder" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter the value of divident&quot;" newline="True"/>
            <comment text="getting input from user"/>
            <input variable="divident"/>
            <output expression="&quot;enter the value of divisor&quot;" newline="True"/>
            <comment text="getting input from user"/>
            <input variable="divisor"/>
            <comment text="dividing divident by divisor"/>
            <assign variable="quotient" expression="divident/divisor"/>
            <output expression="&quot;the quotient is&quot;&amp;quotient" newline="True"/>
            <comment text="doing operations to find remainder"/>
            <assign variable="remainder" expression="divident-(divisor*quotient)"/>
            <output expression="&quot;the remainder is&quot;&amp;remainder" newline="True"/>
            <comment text="renaming the input to easily understand the final output"/>
            <declare name="a, b" type="Real" array="False" size=""/>
            <assign variable="a" expression="divident"/>
            <assign variable="b" expression="divisor"/>
            <output expression="a&amp; &quot;%&quot; &amp;b&amp; &quot;is&quot; &amp;remainder" newline="True"/>
        </body>
    </function>
</flowgorithm>
